{
  "name": "onepage",
  "buildCommand": "npm run build",
  "installCommand": "npm install",
  "outputDirectory": "./out",
  "nodeVersion": "22.11.0",
  "headers": [
    {
      "source": "/*",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "max-age=0"
        }
      ]
    },
    {
      "source": "/assets/*",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "max-age=31536000"
        }
      ]
    }
  ]
}