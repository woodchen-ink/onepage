{
  "name": "onepage",
  "buildCommand": "npm run build",
  "installCommand": "npm install",
  "outputDirectory": "./out",
  "nodeVersion": "22.11.0",
  "rewrites": [
    {
      "source": "/dplayer/*",
      "destination": "/dplayer"
    },
    {
      "source": "/aliplayer/*",
      "destination": "/aliplayer"
    },
    {
      "source": "/ckplayer/*",
      "destination": "/ckplayer"
    },
    {
      "source": "/xgplayer/*",
      "destination": "/xgplayer"
    }
  ],
  "headers": [
    {
      "source": "/*",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Cache-Control",
          "value": "max-age=0"
        }
      ]
    },
    {
      "source": "/assets/*",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "max-age=31536000"
        }
      ]
    }
  ]
}
